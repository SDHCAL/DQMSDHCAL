<lcdqm>

  <ModuleApplication pilot="off" archiver="on">
  
    <Network>
      <StartOfRunService  value="LCDQM/LCIOFileServer/RUN_CONTROL/START_OF_RUN" />
      <EndOfRunService    value="LCDQM/LCIOFileServer/RUN_CONTROL/END_OF_RUN" />       
      <DataService        value="LCDQM/LCIOFileServer/DATA/EVENT" />
      <PublicationService value="LCDQM/TestModule/" />    
    </Network>
    
    <Cycle>
      <CycleType    value="TimerCycle" />
      <CycleValue   value="5" />
      <CycleTimeout value="2" />
    </Cycle>
    
    <Archiver>
      <ArchiveName value="SDHCALDetectorModuleArchive" />
      <ArchiverDirectory   value="."/>
    </Archiver>
    
    <Module name="SDHCALDetectorModule">
      <SleepTime> 0 </SleepTime>
    </Module> 
  
  </ModuleApplication>
  
</lcdqm>